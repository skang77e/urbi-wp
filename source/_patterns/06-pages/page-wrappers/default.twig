{% extends '@layouts/site-container/site-container.twig' %}
  {% block site_content %}

    {% embed '@layouts/header/header.twig' with {
      'has_constrain': true
    } %}
      {% block content %}
        {% include '@components/menu/menu.twig' with {
          'menu_name': 'account',
          'items': [
            { 'title': 'My Account', 'link': '#' },
            { 'title': 'Logout', 'link': '#' }
          ]
        } %}
        {% include '@components/site-name/site-name.twig' %}
      {% endblock %}
    {% endembed %}

    {% embed '@layouts/region/region.twig' with {
      'region_name': 'navigation',
      'has_constrain': true
    } %}
      {% block content %}
        {% include '@components/menu/menu.twig' with {
          'menu_name': 'main',
          'items': [
            { 'title': 'Home', 'link': '#' },
            { 'title': 'About', 'link': '#', 'children': [
              { 'title': 'Submenu item one', 'link': '#' },
              { 'title': 'Submenu item two', 'link': '#', 'children': [
                { 'title': 'Submenu item one', 'link': '#' },
                { 'title': 'Submenu item two', 'link': '#' },
                { 'title': 'Submenu item three', 'link': '#' }
              ]},
              { 'title': 'Submenu item three', 'link': '#' }
            ]},
            { 'title': 'Resources', 'link': '#', 'children': [
              { 'title': 'Submenu item one', 'link': '#' },
              { 'title': 'Submenu item two', 'link': '#' }
            ]},
            { 'title': 'Contact', 'link': '#' }
          ]
        } %}
      {% endblock %}
    {% endembed %}

    {% embed '@layouts/breadcrumb/breadcrumb.twig' with {
      'has_constrain': false
    } %}
      {% block content %}
        {% include '@components/breadcrumb/breadcrumb.twig' with {
          'breadcrumb': [
            { 'url': '#', 'text': 'Home' },
            { 'url': '#', 'text': 'About' },
            { 'text': 'Current Page' }
          ]
        } %}
      {% endblock %}
    {% endembed %}

    <main id="main" class="main" role="main" tabindex="-1">
      {% embed '@layouts/region/region.twig' with {
        'region_name': 'preface',
        'has_constrain': true
      } %}
        {% block content %}

        {% endblock %}
      {% endembed %}

      {% embed '@layouts/content/content.twig' with {
        'has_constrain': true
      } %}
        {% block content %}
          {{ page_default_content }}
        {% endblock %}
      {% endembed %}

      {% embed '@layouts/region/region.twig' with {
        'region_name': 'postscript',
        'has_constrain': true
      } %}
        {% block content %}

        {% endblock %}
      {% endembed %}
    </main>

    {% embed '@layouts/footer/footer.twig' with {
      'has_constrain': true
    } %}
      {% block content %}
        {% include '@components/menu/menu.twig' with {
          'menu_name': 'footer',
          'items': [
            { 'title': 'Careers', 'link': '#' },
            { 'title': 'Contact', 'link': '#' },
            { 'title': 'Credits', 'link': '#' },
            { 'title': 'Terms of Use', 'link': '#' }
          ]
        } %}
        {% include '@components/copyright/copyright.twig' %}
      {% endblock %}
    {% endembed %}

    {% include '@components/back-to-top/back-to-top.twig' with {
      'text': 'Back to top',
      'top_element': 'top'
    }%}

  {% endblock %}
